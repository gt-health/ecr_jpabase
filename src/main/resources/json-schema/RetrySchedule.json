{
	"$schema": "http://json-schema.org/schema#",
	"id": "https://git2.icl.gtri.org/projects/CDCSTI/repos/json-schema/RetySchedule.json#",
	"type": "object",

	"ECRId": {
		"type": "Integer"
	},
	
	"nextRetry": {
		"type": "string",
		"pattern": "[0-9]{4}-[01][0-9]-[0-3][0-9]T[0-2][0-9]:[0-5][0-9]:[0-5][0-9]([+-][0-2][0-9]:[0-5][0-9]|Z)"
	},
	
	"retryIntervalEnabled": {
		"type": "boolean"
	},
	
	"retryInterval": {
		"type": "string"
	},

	"default": {}
}